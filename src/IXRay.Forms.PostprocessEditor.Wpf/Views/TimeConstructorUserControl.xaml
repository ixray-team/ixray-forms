<UserControl x:Class="IXRay.Forms.PostprocessEditor.Wpf.Views.TimeConstructorUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:l="clr-namespace:IXRay.Forms.PostprocessEditor.Wpf"
             mc:Ignorable="d"
             d:DesignHeight="30"
             d:DesignWidth="400">
    <StackPanel Orientation="Horizontal">
        <Label Content="Start(sec)"
               VerticalAlignment="Center" />
        <TextBox Text="0,00"
                 Height="20"
                 Width="100"
                 Margin="2" />

        <Label Content="Length(sec)"
               VerticalAlignment="Center" />
        <TextBox Text="0,00"
                 Height="20"
                 Width="100"
                 Margin="2" />

        <Button Content="+"
                Width="22"
                Height="22"
                Margin="2" />
        <Button Content="-"
                Width="22"
                Height="22"
                Margin="2" />
    </StackPanel>
</UserControl>
